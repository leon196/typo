{
	"animations": "animation/scene.json",
	"fonts": {
	},
	"textures": {
	},
	"geometries": {
	},
	"shaders": {
		"land": {
			"vertexShader": "shader/mesh/land.vert",
			"fragmentShader": "shader/color.frag"
		},
		"leaf": {
			"vertexShader": "shader/mesh/leaf.vert",
			"fragmentShader": "shader/color.frag"
		},
		"cloud": {
			"vertexShader": "shader/mesh/cloud.vert",
			"fragmentShader": "shader/color.frag"
		},
		"tree": {
			"vertexShader": "shader/mesh/tree.vert",
			"fragmentShader": "shader/mesh/tree.frag"
		},
		"grass": {
			"vertexShader": "shader/mesh/grass.vert",
			"fragmentShader": "shader/color.frag",
			"side": 2
		},
		"skybox": {
			"vertexShader": "shader/mesh/skybox.vert",
			"fragmentShader": "shader/mesh/skybox.frag",
			"side": 1
		},
		"text": {
			"vertexShader": "shader/mesh/text.vert",
			"fragmentShader": "shader/mesh/text.frag",
			"side": 2,
			"depthTest": false,
			"transparent": true
		},
		"paint": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/paint.frag"
