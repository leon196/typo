		"terrain": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/terrain.frag"
		},
		"biotope": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/biotope.frag"
		},
		"craft": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/craft.frag"
		},
		"render": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/render.frag"
		}
	}
}
